{
  "name": "puppetfinland-os",
  "version": "1.0.2",
  "author": "Puppet-Finland team",
  "license": "BSD license",
  "summary": "Setup various OS-specific parameters",
  "source": "https://github.com/Puppet-Finland/puppet-os.git",
  "project_page": "https://github.com/Puppet-Finland/puppet-os",
  "dependencies": [ ],
  "requirements": [
    {"name": "puppet", "version_requirement": ">= 4.0.0"}
